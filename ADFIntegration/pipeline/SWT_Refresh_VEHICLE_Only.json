{
	"name": "SWT_Refresh_VEHICLE_Only",
	"properties": {
		"activities": [
			{
				"name": "DB_Transform_Incremental",
				"type": "DatabricksNotebook",
				"dependsOn": [
					{
						"activity": "Set_Run_Date",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"notebookPath": {
						"value": "@{concat('/DataEngineering/SWT/SWT_Data_Ingestion/',pipeline().parameters.table_name)}",
						"type": "Expression"
					},
					"baseParameters": {
						"run_date": {
							"value": "@pipeline().parameters.run_date",
							"type": "Expression"
						},
						"table_name": {
							"value": "@pipeline().parameters.table_name",
							"type": "Expression"
						},
						"container": {
							"value": "@pipeline().parameters.container",
							"type": "Expression"
						}
					}
				},
				"linkedServiceName": {
					"referenceName": "MBUSA_DEV",
					"type": "LinkedServiceReference"
				}
			},
			{
				"name": "Check_If_File_Exist",
				"type": "IfCondition",
				"dependsOn": [
					{
						"activity": "DB_Transform_Incremental",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"userProperties": [],
				"typeProperties": {
					"expression": {
						"value": "@equals(activity('DB_Transform_Incremental').output.runOutput,'Success')",
						"type": "Expression"
					},
					"ifTrueActivities": [
						{
							"name": "Drop_If_Staging_Table_Exist",
							"type": "SqlServerStoredProcedure",
							"dependsOn": [],
							"policy": {
								"timeout": "7.00:00:00",
								"retry": 0,
								"retryIntervalInSeconds": 30,
								"secureOutput": false,
								"secureInput": false
							},
							"userProperties": [],
							"typeProperties": {
								"storedProcedureName": {
									"value": "[ADW_VISTA_EDW_STG].[DROP_STG_TBL_SWT]",
									"type": "Expression"
								},
								"storedProcedureParameters": {
									"TABLE_NAME": {
										"value": {
											"value": "@pipeline().parameters.table_name",
											"type": "Expression"
										},
										"type": "String"
									}
								}
							},
							"linkedServiceName": {
								"referenceName": "AzureSqlDW2",
								"type": "LinkedServiceReference"
							}
						},
						{
							"name": "Create_Staging_Table",
							"type": "SqlServerStoredProcedure",
							"dependsOn": [
								{
									"activity": "Drop_If_Staging_Table_Exist",
									"dependencyConditions": [
										"Succeeded"
									]
								}
							],
							"policy": {
								"timeout": "7.00:00:00",
								"retry": 0,
								"retryIntervalInSeconds": 30,
								"secureOutput": false,
								"secureInput": false
							},
							"userProperties": [],
							"typeProperties": {
								"storedProcedureName": {
									"value": "@concat(pipeline().parameters.schema_name,'.','CREATE_STG_TBL')",
									"type": "Expression"
								},
								"storedProcedureParameters": {
									"TABLE_NAME": {
										"value": {
											"value": "@pipeline().parameters.table_name",
											"type": "Expression"
										},
										"type": "String"
									}
								}
							},
							"linkedServiceName": {
								"referenceName": "AzureSqlDW2",
								"type": "LinkedServiceReference"
							}
						},
						{
							"name": "Load_Staging_Data",
							"type": "Copy",
							"dependsOn": [
								{
									"activity": "Create_Staging_Table",
									"dependencyConditions": [
										"Succeeded"
									]
								}
							],
							"policy": {
								"timeout": "7.00:00:00",
								"retry": 0,
								"retryIntervalInSeconds": 30,
								"secureOutput": false,
								"secureInput": false
							},
							"userProperties": [],
							"typeProperties": {
								"source": {
									"type": "DelimitedTextSource",
									"storeSettings": {
										"type": "AzureBlobFSReadSettings",
										"recursive": true,
										"wildcardFolderPath": {
											"value": "@concat('processed/',pipeline().parameters.table_name,'/year=',substring(pipeline().parameters.run_date,0,4),'/month=',substring(pipeline().parameters.run_date,5,2),'/day=',substring(pipeline().parameters.run_date,8,2))",
											"type": "Expression"
										},
										"wildcardFileName": "*.csv",
										"enablePartitionDiscovery": false
									},
									"formatSettings": {
										"type": "DelimitedTextReadSettings"
									}
								},
								"sink": {
									"type": "SqlDWSink",
									"writeBehavior": "Insert",
									"sqlWriterUseTableLock": false
								},
								"enableStaging": false
							},
							"inputs": [
								{
									"referenceName": "SWT_processed",
									"type": "DatasetReference",
									"parameters": {
										"container": {
											"value": "@pipeline().parameters.container",
											"type": "Expression"
										}
									}
								}
							],
							"outputs": [
								{
									"referenceName": "base_pattern_dw",
									"type": "DatasetReference",
									"parameters": {
										"schema_name": {
											"value": "@pipeline().parameters.schema_name",
											"type": "Expression"
										},
										"table_name": {
											"value": "@if(endswith(pipeline().parameters.table_name,'_UCARS' ),pipeline().parameters.table_name,concat(pipeline().parameters.table_name,'_SWT'))",
											"type": "Expression"
										}
									}
								}
							]
						},
						{
							"name": "Historical_update",
							"description": "This procedure will update below columns with SWT vehicle data \nCDE_MM_STAT,CDE_SD_STAT,CDE_SWT_VEH_AVAIL,DTE_MM_STAT,DTE_SD_STAT,NUM_SWT_INT_VEH_ID ,NUM_SWT_INT_MOD,DTS_DW_UPDT,ID_DW_UPDT_USER ",
							"type": "SqlServerStoredProcedure",
							"dependsOn": [
								{
									"activity": "Load_Staging_Data",
									"dependencyConditions": [
										"Succeeded"
									]
								}
							],
							"policy": {
								"timeout": "7.00:00:00",
								"retry": 0,
								"retryIntervalInSeconds": 30,
								"secureOutput": false,
								"secureInput": false
							},
							"userProperties": [],
							"typeProperties": {
								"storedProcedureName": "[ADW_VISTA_EDW_STG].[VEHICLE_HISTORICAL_UPDATE]"
							},
							"linkedServiceName": {
								"referenceName": "AzureSqlDW2",
								"type": "LinkedServiceReference"
							}
						}
					]
				}
			},
			{
				"name": "Set_Run_Date",
				"type": "SetVariable",
				"dependsOn": [],
				"userProperties": [],
				"typeProperties": {
					"variableName": "run_date",
					"value": {
						"value": "@formatDateTime(\n    coalesce(\n            pipeline().parameters.run_date,\n            convertTimeZone(\n                utcNow(),\n                'UTC',\n                'Eastern Standard Time'\n            )\n    ),\n'yyyy-MM-dd'\n)",
						"type": "Expression"
					}
				}
			}
		],
		"parameters": {
			"schema_name": {
				"type": "string",
				"defaultValue": "ADW_VISTA_EDW_STG"
			},
			"table_name": {
				"type": "string",
				"defaultValue": "VEHICLE"
			},
			"run_date": {
				"type": "string"
			},
			"container": {
				"type": "string",
				"defaultValue": "adls-vehicle-swt"
			}
		},
		"variables": {
			"run_date": {
				"type": "String"
			}
		},
		"folder": {
			"name": "SWT_Data_Ingestion"
		},
		"annotations": []
	}
}